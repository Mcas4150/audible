

<div class="container-fluid">


<div class="row">
  <% @events.each do |event| %>
  <div class="col-xs-12">
    <div class="row">
      <div class="col-xs-2"><%= event.datetime %></div>
      <div class="col-xs-3"><%= event.name %></div>
      <div class="col-xs-2"><%= event.location %></div>
      <div class="col-xs-2"><%= event.note %></div>
      <div class="col-xs-3"><%= event.image %></div>
    </div>
  </div>
  <%end%>

</div>


</div>
