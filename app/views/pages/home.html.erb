<div class="container-fluid" style="padding-right:10px; margin:5px;">

<div class= "row">
  <div class="col-xs-12 col-sm-8 draggable" style="border">


  <% @events.each do |event| %>

    <div  class="row boxy">
      <div class="col-xs-2 col-sm-2"><%= event.date %></div>
      <div class="col-xs-3 col-sm-6" style="display:flex; flex-direction: row; align-items: center;"><%= event.name %></div>
      <div class="col-xs-2 col-sm-3 " style="display:flex; flex-direction: row; align-items: : center;"><%= event.location %></div>

    </div>

  <%end%>

</div>
