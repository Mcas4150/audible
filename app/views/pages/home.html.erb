

<div class="container-fluid" style="padding-right:10px;">


<div class="row">
  <% @events.each do |event| %>
  <div class="col-12 boxy">
    <div class="row">
      <div class="col-xs-2 col-sm-2"><%= event.date %></div>
      <div class="col-xs-3 col-sm-5"><%= event.name %></div>
      <div class="col-xs-2 col-sm-2 "><%= event.location %></div>
      <div class="col-xs-2 col-sm-2"><%= event.note %></div>
      <div class="col-xs-12 col-sm-1" style="display:flex; flex-direction: row; justify-content: flex-end; cursor: zoom-in; width: 75px;"><img id="myImg" src = '<%= event.image %>' style= "width: 100%; height: 100%;">
          <div id="myModal" class="modal">

  <!-- The Close Button -->
            <span class="close" onclick="document.getElementById('myModal').style.display='none'">&times;</span>

            <!-- Modal Content (The Image) -->
            <img class="modal-content" id="img01" style="border-radius: 10px;">

            <!-- Modal Caption (Image Text) -->
            <div id="caption"></div>
          </div>
      </div>
    </div>
  </div>
  <%end%>

</div>


</div>
